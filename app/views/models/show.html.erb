<div class="bg-white border-b border-[#EBEEF5]">
  <model-detail
    :tags="<%= @tags %>"
    local-model-id="<%= @local_model.id %>"
    :model="<%= @model %>"
    :local-model="<%= @local_model.to_json %>"
    :last-commit="<%= @last_commit || {} %>"
    :branches="<%= @branches %>"
    current-branch="<%= @current_branch || 'main' %>"
    current-path="<%= @current_path || '' %>"
    default-tab="<%= @default_tab || 'summary' %>"
    :content="<%= @content || {} %>"
    :action-name="'<%= action_name %>'"
    :settings-visibility="<%= @settings_visibility %>"
    :owner-url="'<%= @owner_url %>'"
    avatar="<%= @avatar_url %>"
    :can-write="<%= current_user&.can_write?(@local_model) %>"
  ></model-detail>
</div>
